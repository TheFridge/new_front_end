<div class='span6'>
  <div class='hero-unit'>
    <h1>Shopping List</h1>
    <div class='list'>
      <% JSON.parse(list.body)["list_ingredients"].each do |ingredient| %>
        <p><%= ingredient["raw_name"] %></p>
      <% end %>
    </div>
    <%= button_to "I've got this shiz", populate_cupboard_path %>
  </div>
</div>
