<div class='span6'>
  <div class='hero-unit'>
    <h1>Shopping List</h1>
    <div class='list'>
      <% list["list_ingredients"].each do |ingredient| %>
        <p><%= ingredient["raw_name"] %></p>
      <% end %>
      <%= button_to "Put Away Groceries", populate_cupboard_path(list_id: list['id']) %>
    </div>
  </div>
</div>
